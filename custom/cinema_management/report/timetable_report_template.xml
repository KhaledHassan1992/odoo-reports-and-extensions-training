<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="timetable_report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2>Timetable Report</h2>
                <br></br>
                <span>Date:</span>
                <span t-field="doc.date"/>
                <br></br>
                <span>Movie:</span>
                <span t-field="doc.movie"/>
                <br></br>
                <span>Total Seats:</span>
                <span t-field="doc.total_seats"/>
                <br></br>
                <span>Sold Seats:</span>
                <span t-field="doc.sold_seats"/>
                <br></br>
                <span>Remaining Seats:</span>
                <span t-field="doc.remaining_seats"/>
            </t>
        </t>
    </template>
</odoo>